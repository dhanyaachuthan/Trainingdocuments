// Le code dans "draw" est ex\'e9cut\'e9 \'e0 chaque frame, pour faire un dessin.\par
function draw() \{\par
  // Remplir le dessin d'une couleur unique.\par
  // Un seul nombre = nuance de gris\par
  background(108);\par
  // Trois nombres : RGB\par
  // background(184,150,57);\par
  // Deux nombres : nuance de gris + opacit\'e9\par
  // background(0,5);\par
  // Quatre nombres : RGB + opacit\'e9\par
  // background(255,0,0,2);\par
  \par
 \par
  stroke(0);\par
  strokeWeight(1);\par
  // Dessiner le pixel qui est \'e0 100 pixels du bord gauche et 200 pixels du bord haut\par
  point(100,200);\par
  \par
  // Changer la couleur du crayon\par
  stroke(255,0,0);\par
  // Changer la taille du crayon\par
  strokeWeight(5);\par
  point(200,100);\par
  \par
  // Dessiner une ligne\par
  line(50, 320, 10, 50);\par
  \par
  // Couleur de remplissage\par
  // fill(0,255,0);\par
  noFill();\par
  // Dessiner un cercle\par
  circle(300,200,100);\par
  \par
  // Faire un carr\'e9\par
  square(200,200,50);\par
  \par
  // Faire un rectangle\par
  rect(250,50,100,20);\par
  \par
  // Faire une ellipse\par
  ellipse(150,300,50,100);\par
  \par
  // console.log("Draw");\par
  // Variables cr\'e9\'e9es automatiquement par p5\par
  // console.log(mouseX, mouseY);\par
  circle(mouseX,mouseY,50);\par
\}\par
